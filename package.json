{
  "name": "@oneappexchange/nestjs-uws",
  "version": "1.3.0",
  "description": "NestJS UWebSocket.js adapter",
  "author": "Vladislav Len <len.vladislavs@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "npx tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "websockets",
    "uwebsocket"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OneAppExchange/nestjs-uws"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.2.0",
    "@nestjs/websockets": "^8.2.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.4.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/platform-express": "^8.2.0",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.2.0",
    "@nestjs/websockets": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/supertest": "2.0.11",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "rxjs": "^7.4.0",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsc-watch": "4.5.0",
    "tsconfig-paths": "^3.11.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.4.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
